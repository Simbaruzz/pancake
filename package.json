{
  "name": "hypetype",
  "productName": "hypetype",
  "version": "1.0.0",
  "description": "Flow symbol type app",
  "main": "app-build/main.js",
  "type": "module",
  "scripts": {
    "start": "tsc && electron .",
    "build": "tsc && electron-builder",
    "dev": "tsc --watch & electron .",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "build": {
    "appId": "ru.hypetype.keyboard",
    "artifactName": "${productName}.${ext}",
    "productName": "hypetype",
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "dependencies": {
    "@jitsi/robotjs": "^0.6.13",
    "auto-launch": "^5.0.6",
    "electron-is-dev": "^3.0.1",
    "electron-updater": "^6.3.9",
    "open": "^10.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/auto-launch": "^5.0.5",
    "@types/electron": "^1.4.38",
    "@types/node": "^22.9.1",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "typescript": "^5.7.2"
  }
}
